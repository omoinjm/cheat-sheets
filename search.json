---
layout: none
---
[
  {% assign all_docs = site.pages | concat: site.documents %}
  {% for doc in all_docs %}
    {% if doc.title and doc.url != "/search.json" and doc.url != "/search/" %}
    {
      "title": {{ doc.title | jsonify }},
      "url": {{ doc.url | relative_url | jsonify }},
      "content": {{ doc.content | strip_html | strip_newlines | downcase | jsonify }}
    } {% unless forloop.last %},{% endunless %}
    {% endif %}
  {% endfor %}
]
